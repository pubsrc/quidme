# Non-sensitive defaults for dev
project_name                  = "payme"
aws_region                    = "eu-west-2"
# Deprecated: kept for backward compatibility. Runtime/frontend config is now derived in Terraform.
# parameter_prefix              = "/payme/dev"
service_fee_bps_default       = 500
payme_base_url_default        = "https://dev.quidme.uk"
account_refresh_url_default   = "https://dev.quidme.uk/app/profile"
account_return_url_default    = "https://dev.quidme.uk/app/profile"
cognito_domain_prefix_default = "payme-dev"
callback_urls_default         = ["https://dev.quidme.uk/callback"]
logout_urls_default           = ["https://dev.quidme.uk"]
# frontend_domain_aliases is managed in constants.tf
api_domain_name               = "api.dev.quidme.uk"
cors_allowed_origins_default  = ["https://dev.quidme.uk"]

# Optional: provide existing cert ARNs.
# CloudFront frontend cert must be in us-east-1.
# API Gateway regional custom domain cert must be in eu-west-2.
# frontend_acm_certificate_arn = "arn:aws:acm:us-east-1:123456789012:certificate/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
# api_acm_certificate_arn      = "arn:aws:acm:eu-west-2:123456789012:certificate/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"

# Optional: override placeholder secrets for first apply
# stripe_secret_placeholder         = "sk_test_..."
# stripe_webhook_secret_placeholder = "whsec_..."
# google_client_id_placeholder      = "..."
# google_client_secret_placeholder  = "..."
